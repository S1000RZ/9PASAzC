local Server = syn.request({
   Url = "http://127.0.0.1/Whitelist/Server.php?Key="..getgenv().key.."&HWID="..game:GetService("RbxAnalyticsService"):GetClientId(),
   Method = "GET" 
}).Body

if Server == 'WHITELIST!' then
   print('script loadded')
elseif Server == "Invaid Hwid !" then
   print('Invaid Hwid')
elseif Server == "Invaid Key !" then
   print('Invaid Key')
end
